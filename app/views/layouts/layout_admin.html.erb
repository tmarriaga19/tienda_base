<!DOCTYPE html>
<html>
  <head>
    <title>TiendaBase</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>   
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
  <div class="container">
    <div class="row">    
        <%= render 'layouts/encab' %>
    </div>
     <div class="row">
          <div class="col-lg-3 ">
            <%= render 'layouts/navegador' %>
          </div>
          <div class="col-lg-9">
              <%= yield %>
          </div>
      </div>
  </div>  
  </body>
</html>
