<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table class="table-responsive">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Identificacion</th>
      <th>Idtipoidentificacion</th>
      <th>Razonsocial</th>
      <th>Correo</th>
      <th>Direccion</th>
      <th>Telefonos</th>
      <th>Idtipocliente</th>
      <th>Idcategoriacliente</th>
      <th>Idestado</th>
      <th>Idgenero</th>
      <th>Eliminado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nombres %></td>
        <td><%= cliente.apellidos %></td>
        <td><%= cliente.identificacion %></td>
        <td><%= cliente.idtipoidentificacion %></td>
        <td><%= cliente.razonSocial %></td>
        <td><%= cliente.correo %></td>
        <td><%= cliente.direccion %></td>
        <td><%= cliente.telefonos %></td>
        <td><%= cliente.idtipocliente %></td>
        <td><%= cliente.idcategoriacliente %></td>
        <td><%= cliente.idestado %></td>
        <td><%= cliente.idgenero %></td>
        <td><%= cliente.eliminado %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
